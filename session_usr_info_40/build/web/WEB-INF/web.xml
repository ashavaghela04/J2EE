<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>sessiojn_servlet_1</servlet-name>
        <servlet-class>sessiojn_servlet_1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>session_servlet_2</servlet-name>
        <servlet-class>session_servlet_2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>session_logout</servlet-name>
        <servlet-class>session_logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>sessiojn_servlet_1</servlet-name>
        <url-pattern>/sessiojn_servlet_1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>session_servlet_2</servlet-name>
        <url-pattern>/session_servlet_2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>session_logout</servlet-name>
        <url-pattern>/session_logout</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
